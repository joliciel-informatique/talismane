<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="build" name="talismane-all">
    <import file="ant/global.xml"/>
	<target name="build">
		<subant target="jar">
			<fileset dir=".">
				<include name="talismane_utils/build.xml"/>
			</fileset>
		</subant>
		<subant target="jar">
			<fileset dir=".">
				<include name="talismane_core/build.xml"/>
			</fileset>
		</subant>
		<subant target="jar">
			<fileset dir=".">
				<include name="talismane_lefff/build.xml"/>
				<include name="talismane_ftb/build.xml"/>
				<include name="talismane_ftbDep/build.xml"/>
			</fileset>
		</subant>
		<subant target="jar">
			<fileset dir=".">
				<include name="talismane_fr/build.xml"/>
				<include name="talismane_trainer_fr/build.xml"/>
			</fileset>
		</subant>
	</target>
</project>
