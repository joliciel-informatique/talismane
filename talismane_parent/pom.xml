	<name>Talismane</name>
	<organization>
		<name>Joliciel Informatique SARL</name>
		<url>http://www.joli-ciel.com/</url>
	</organization>
	<scm>
		<developerConnection>urieli</developerConnection>
		<url>https://github.com/urieli/talismane</url>
		<connection>https://github.com/urieli/talismane.git</connection>
		<tag>v${project.version}</tag>
	</scm>
	<url>https://github.com/urieli/talismane</url>
